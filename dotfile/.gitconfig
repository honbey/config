[user]
  name = ${name}
  email = ${email}
  signingKye = ${email}
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[core]
  quotepath = false
  safecrlf = true
  # Unix / Linux 
  autocrlf = input
  # Windows 
  # autocrlf = true
[init]
  defaultBranch = main
